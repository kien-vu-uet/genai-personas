{
    "theme": "Dracula",
    "selectedAuthType": "gemini-api-key",
    "preferredEditor": "vscode",
    "maxSessionTurns": 15,
    "mcpServers": {
        "playwright": {
            "type": "stdio",
            "command": "npx",
            "args": [
                "@playwright/mcp@latest"
            ],
            "gallery": true
        },
        "brainstormWithExperts": {
            "type": "http",
            "httpUrl": "http://localhost:8976/mcp/"
        },
        "grafana": {
            "command": "docker",
            "args": [
                "run",
                "--rm",
                "-i",
                "-e",
                "GRAFANA_URL",
                "-e",
                "GRAFANA_API_KEY",
                "mcp/grafana",
                "-t",
                "stdio",
                "-debug"
            ],
            "env": {
                "GRAFANA_URL": "${GRAFANA_URL}",
                "GRAFANA_API_KEY": "${GRAFANA_API_KEY}"
            }
        }
    }
}